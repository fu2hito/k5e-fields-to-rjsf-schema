{
  "name": "k5e-fields-to-rjsf-schema",
  "version": "2.0.0",
  "description": "kintoneアプリのフィールド情報をrjsfのJSONスキーマに変換するライブラリ",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "biome lint --write src",
    "format": "biome format --write src",
    "check": "biome check  --write src",
    "fmt:md": "dprint fmt ./**/*.md",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "kintone",
    "rjsf",
    "json-schema",
    "form"
  ],
  "author": "fu2hito",
  "license": "Apache-2.0",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rjsf/mui": "^5.24.8",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^5.7.1",
    "@rjsf/core": "^5.24.8",
    "@rjsf/utils": "^5.24.8",
    "@rjsf/validator-ajv8": "^5.24.8"
  }
}